{
  "name": "timestudy-backend",
  "dockerComposeFile": "../docker-compose.yaml",
  "service": "backend",
  "workspaceFolder": "/app",

  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python",
        "terminal.integrated.defaultProfile.linux": "bash"
